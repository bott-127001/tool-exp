# Upstox OAuth Credentials for Samarth
UPSTOX_SAMARTH_CLIENT_ID=your_samarth_client_id_here
UPSTOX_SAMARTH_CLIENT_SECRET=your_samarth_client_secret_here

# Upstox OAuth Credentials for Prajwal
UPSTOX_PRAJWAL_CLIENT_ID=your_prajwal_client_id_here
UPSTOX_PRAJWAL_CLIENT_SECRET=your_prajwal_client_secret_here

# OAuth Redirect URI (must match exactly what you configured in Upstox app settings)
UPSTOX_REDIRECT_URI=http://localhost:8000/api/auth/callback

# Upstox Login Credentials for Automated OAuth (Backend Automation)
# Phone number used to login to Upstox
UPSTOX_SAMARTH_PHONE=your_samarth_phone_number
# TOTP secret (base32 encoded) from your authenticator app
UPSTOX_SAMARTH_TOTP_SECRET=your_samarth_totp_base32_secret
# 6-digit Upstox PIN
UPSTOX_SAMARTH_PIN=your_6_digit_pin

UPSTOX_PRAJWAL_PHONE=your_prajwal_phone_number
UPSTOX_PRAJWAL_TOTP_SECRET=your_prajwal_totp_base32_secret
UPSTOX_PRAJWAL_PIN=your_6_digit_pin

# Frontend Dashboard Authentication (Separate from Upstox)
# Passwords for accessing the dashboard (will be hashed on first run)
FRONTEND_SAMARTH_PASSWORD=your_frontend_password_for_samarth
FRONTEND_PRAJWAL_PASSWORD=your_frontend_password_for_prajwal

# MongoDB Connection
MONGO_URI=your_mongodb_connection_string